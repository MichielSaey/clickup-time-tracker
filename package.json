{
  "private": true,
  "version": "0.2.0",
  "name": "clickup-time-tracker",
  "productName": "Time Tracker",
  "description": "A tool for painlessly tracking time on Clickup tasks",
  "repository": {
    "type": "git",
    "url": "https://github.com/gwleuverink/clickup-time-tracker"
  },
  "author": {
    "name": "Willem Leuverink",
    "email": "willem@leuver.ink",
    "url": "https://leuver.ink"
  },
  "scripts": {
    "lint": "vue-cli-service lint",
    "build:osx": "npm run electron:build -p never",
    "build:win": "npm run electron:build -p never -- --win --x64",
    "build:linux": "npm run electron:build -p never -- --linux",
    "build:all": "npm run build:osx && npm run build:win && npm run build:linux",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@heroicons/vue": "^1.0.5",
    "@sentry/electron": "^3.0.7",
    "core-js": "^3.23.3",
    "electron-store": "^8.0.2",
    "electron-updater": "^5.0.1",
    "naive-ui": "^2.30.0",
    "sass": "^1.53.0",
    "v-offline": "^3.1.0",
    "vue": "^3.0.0",
    "vue-cal": "^4.2.0",
    "vue-router": "^4.0.0-0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/compiler-sfc": "^3.0.0",
    "autoprefixer": "^10.4.7",
    "babel-eslint": "^10.1.0",
    "electron": "^18.0.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^7.5.0",
    "eslint-plugin-vue": "^9.1.1",
    "postcss": "^8.4.14",
    "sass-loader": "^10.2.1",
    "tailwindcss": "^3.1.4",
    "vue-cli-plugin-electron-builder": "~2.1.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
